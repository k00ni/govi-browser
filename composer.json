{
    "name": "k00ni/govi-browser",
    "description": "Browser for the Global Ontology and Vocabulary Index (GOVI).",
    "type": "project",
    "keywords": ["RDF", "Semantic Web", "Linked Data", "Linked Open Data", "Ontology", "Vocabulary"],
    "license": "GPL-2.0-only",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/asset": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49.0",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-strict-rules": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
