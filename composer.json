    "type": "project",
    "keywords": ["RDF", "Semantic Web", "Linked Data", "Linked Open Data", "Ontology", "Vocabulary"],
    "license": "GPL-2.0-only",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "symfony/cache": "^7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49.0",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-strict-rules": "^1.1"
    },
    "autoload": {
        "files": [
            "functions.php"
